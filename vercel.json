{
    "version": 2,
    "env": {
        "PORT": "3000"
    },
    "buildCommand": "nest build",
    "devCommand": "nest start",
    "installCommand": "npm install",
    "outputDirectory": "./",
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET"
            ] 
        }
    ]
}
