{
    "version": 2,
    "env": {
        "PORT": 3000
    },
    "buildCommand": "npm run build",
    "build": {
        "src": "src/main.ts",
        "use": "@vercel/node"
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET"
            ] 
        }
    ]
}
